{
    "window.zoomLevel": 0,
    "git.enableSmartCommit": true,
    "editor.fontSize": 19,
    "files.associations": {
        "*.makefile": "makefile"
    },
    "editor.cursorStyle": "line-thin",
    "editor.minimap.showSlider": "always",
    "editor.minimap.renderCharacters": false,
    "editor.mouseWheelScrollSensitivity": 2,
    // "editor.formatOnType": true,
    "files.autoSave": "onFocusChange",
    "editor.wordWrap": "on",
    // "workbench.colorTheme": "Monokai",
    // "window.openFilesInNewWindow": "on",
    // "window.openFoldersInNewWindow": "on",
    "window.restoreFullscreen": true,
    "window.menuBarVisibility": "default",
    "zenMode.hideStatusBar": false,
    "zenMode.restore": true,
    //debug
    "debug.inlineValues": true,
    "editor.smoothScrolling": true,
    "editor.minimap.enabled": true,
    // "editor.formatOnPaste": true,
    // "editor.formatOnSave": true,
    "editor.cursorBlinking": "solid",
    "editor.mouseWheelZoom": true,
    "editor.fontLigatures": true,
    "editor.showFoldingControls": "always",
    // "editor.fontFamily": "Ubuntu Italic"
    // "editor.fontFamily": "Tlwg Typo Regular"
    // "editor.fontFamily": "Times Ne  w Roman"
    // "terminal.integrated.rightClickCopyPaste": true,
    "terminal.integrated.fontFamily": "'Droid Sans Mono', 'Courier New', monospace, 'Droid Sans Fallback'",
    "terminal.integrated.cursorBlinking": true,
    "debug.internalConsoleOptions": "neverOpen",
    "workbench.quickOpen.closeOnFocusLost": false,
    "editor.stablePeek": true,
    "typescript.useCodeSnippetsOnMethodSuggest": true,
    "editor.tabCompletion": true,
    "explorer.confirmDragAndDrop": false,
    "workbench.activityBar.visible": true,
    // "workbench.panel.location": "bottom",
    "editor.multiCursorModifier": "ctrlCmd",
    // "C_Cpp.clang_format_fallbackStyle": "Google",
    "explorer.confirmDelete": false,
    "workbench.sideBar.location": "left",
    "editor.renderControlCharacters": true,
    "editor.useTabStops": false,
    "workbench.editor.showTabs": true,
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "open-in-browser.default": "Opera",
    "editor.quickSuggestions":{
        "other": true,
        "comments": true,
        "strings": true
    },
    "editor.snippetSuggestions": "inline",
    "editor.suggestFontSize": 25,
    // "editor.suggestSelection":"recentlyUsedByPrefix", 
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.wordBasedSuggestions": false,
    "workbench.colorTheme": "Monokai",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "terminal.integrated.fontSize": 22
}